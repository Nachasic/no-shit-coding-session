{
  "name": "ts-semver-tpl",
  "version": "0.0.0-semantically-released",
  "description": "Template project",
  "main": "dist/index.js",
  "ts:main": "src/index.ts",
  "typings": "src/index.ts",
  "files": [
    "dist/**/*.js",
    "src/**/*.ts"
  ],
  "author": "Nachasic <nachasic@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npx ts-node fuse.ts",
